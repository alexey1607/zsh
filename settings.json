        "**/vars/**/*.yml": "jinja-yaml",
        "**/tasks/**/*.yml": "ansible",
        "**/handlers/*.yml": "ansible",
        "**/meta/*.yml": "ansible",
        "**/roles/**/*.yml": "ansible",
        "**/playbooks/**/*.yml": "ansible",
        "**/ansible/**/hosts": "ini",
        "**/ansible/**/inventory": "ini",
        "ansible.cfg": "ini",
    },
    

