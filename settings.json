    "editor.quickSuggestions": true,

    "yaml.schemas": {
        "kubernetes": "Pod.yaml, Deployment.yaml, Ingress.yaml, ReplicaSet.yaml, Job.yaml",
        "ansible": ["^/roles/*/tasks/*.yaml", "^/tasks/*.yaml", "^/playbooks/*.yaml"],
    },

    "files.associations": {
        // Ansible
        "**/defaults/**/*.yml": "jinja-yaml",
